# https://kubernetes.io/docs/concepts/services-networking/ingress/
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: application-ingress
spec:
  ingressClassName: nginx
  rules:
  - host: frontend.dofreshmind.com
    https:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name:  frontend-service
            port:
              number: 80
---
